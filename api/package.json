{
  "name": "austrac-api",
  "version": "0.5.0",
  "description": "Edge API for austrac on the blockchain",
  "main": "index.js",
  "engines": {
    "node": "8.x"
  },
  "scripts": {
    "start": "node index.js",
    "build": "yarn docs && yarn contracts:compile",
    "dev": "./node_modules/.bin/nodemon index.js",
    "test": "./node_modules/.bin/mocha --timeout 120000 --compilers js:babel-register --require babel-polyfill",
    "test:coverage": "nyc --reporter=html --reporter=text npm test",
    "deps": "./node_modules/babel-cli/bin/babel-node.js ./bin/deps.js",
    "ci": "yarn deps && yarn db:migrate && yarn contracts:deploy && yarn test",
    "lint": "eslint src/**/*.js",
    "docs": "./node_modules/.bin/apidoc -i src/ -o build/docs",
    "up": "npm run docker:up && ./scripts/tcp-port-wait localhost 5432 && npm run db:migrate && npm run contracts:compile && npm run contracts:deploy && npm run docs",
    "up:seed": "concurrently 'yarn run up && yarn run dev' 'yarn dev:seed'",
    "dev:seed": "wait-on http://localhost:5000/health && API_URL=http://localhost:5000 yarn api:seed",
    "test:seed": "wait-on https://austrac.api.demo.commbanklabs.com.au/health && API_URL=https://austrac.api.demo.commbanklabs.com.au yarn api:seed",
    "down": "npm run docker:down",
    "contracts:compile": "./node_modules/babel-cli/bin/babel-node.js ./node_modules/gulp/bin/gulp.js compile",
    "contracts:deploy": "./node_modules/babel-cli/bin/babel-node.js ./bin/deploy.js",
    "docker:up": "docker-compose up -d ethermint postgres",
    "docker:down": "docker-compose down",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:rollback": "node_modules/.bin/sequelize db:migrate:undo",
    "db:rollback:all": "node_modules/.bin/sequelize db:migrate:undo:all",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "db:refresh": "npm run db:rollback:all && npm run db:migrate",
    "api:seed": "babel-node bin/import import/users.toml",
    "precommit": "lint-staged",
    "bootstrap": "yarn run db:rollback:all && yarn run db:migrate && yarn run contracts:deploy && yarn run test:seed"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "keywords": [
    "austrac-api",
    "api",
    "koa",
    "koa2",
    "boilerplate",
    "es6",
    "mongoose",
    "passportjs"
  ],
  "author": "Dan Turner <dan.turner@cba.com.au>",
  "license": "MIT",
  "apidoc": {
    "title": "austrac-api",
    "url": "https://austract.api.commbanklabs.com.au"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xharmagne/project-vision"
  },
  "dependencies": {
    "apidoc": "^0.17.5",
    "async": "^2.3.0",
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
    "chai-things": "^0.2.0",
    "change-case": "^3.0.1",
    "csvtojson": "^1.1.6",
    "faker": "^3.1.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.8",
    "invariant": "^2.2.2",
    "joi": "^10.4.1",
    "json2csv": "^3.7.3",
    "jsonwebtoken": "^7.3.0",
    "kcors": "^2.1.1",
    "koa": "^2.2.0",
    "koa-better-body": "^3.0.2",
    "koa-convert": "^1.2.0",
    "koa-logger": "^2.0.0",
    "koa-mount": "^1.3.0",
    "koa-passport": "^2.2.2",
    "koa-router": "^7.1.1",
    "koa-static": "^2.0.0",
    "koa-validate": "^1.0.7",
    "lodash": "^4.17.4",
    "memory-cache": "^0.1.6",
    "moment": "^2.18.1",
    "node-ethers": "^3.0.4",
    "node-uuid": "^1.4.8",
    "nyc": "^10.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "qs": "^6.4.0",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "solc": "0.4.10",
    "superagent": "^3.5.2",
    "through2": "^2.0.3",
    "wait-for-port": "^0.0.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.13.3",
    "lint-staged": "^7.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.1.0",
    "sinon": "^2.3.4",
    "sleep-promise": "^2.0.0",
    "supertest": "^3.0.0",
    "toml": "^2.3.2",
    "wait-on": "^2.0.2"
  },
  "nyc": {
    "exclude": [
      "build",
      "test",
      "bin",
      "config",
      "src/services",
      "src/modules/index.js",
      "src/models/index.js",
      "src/models/user.js"
    ],
    "check-coverage": true,
    "statements": 90,
    "branches": 80,
    "functions": 90,
    "lines": 90
  }
}
